{
    "name": "recursion-viewer",
    "displayName": "Recursion Viewer",
    "description": "VS Code extension which helps visualize recursion calls tree to make it easier to understand",
    "version": "1.0.2",
    "icon": "assets/icon256.png",
    "homepage": "https://github.com/dmytrobaida/recursion-viewer/tree/main/apps/extension#readme",
    "bugs": "https://github.com/dmytrobaida/recursion-viewer/issues",
    "repository": {
        "type": "git",
        "url": "https://github.com/dmytrobaida/recursion-viewer/tree/main/apps/extension"
    },
    "license": "MIT",
    "publisher": "DmytroBaida",
    "categories": [
        "Other"
    ],
    "keywords": [
        "Recursion",
        "Visualization",
        "Tree",
        "Graph",
        "TypeScript",
        "JavaScript"
    ],
    "type": "commonjs",
    "main": "./index.cjs",
    "engines": {
        "vscode": "^1.85.0"
    },
    "activationEvents": [
        "onLanguage:typescript",
        "onLanguage:javascript"
    ],
    "contributes": {
        "commands": [
            {
                "command": "recursion-viewer.generate",
                "title": "Generate recursion tree"
            }
        ]
    }
}