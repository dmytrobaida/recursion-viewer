{
    "name": "@recursion-viewer/extension",
    "displayName": "recursion-viewer",
    "description": "VS Code extension which helps visualize recursion calls tree to make it easier to understand",
    "version": "0.0.1",
    "type": "commonjs",
    "main": "./index.cjs",
    "engines": {
        "vscode": "^1.85.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:typescript",
        "onLanguage:javascript"
    ],
    "contributes": {
        "commands": [
            {
                "command": "recursion-viewer.generate",
                "title": "Generate recursion tree"
            }
        ]
    }
}